			section.hero.hero--main
				.container
					.hero__inner
						.hero__left
							h1.hero__title Ремонт компьютеров #[strong в день обращения ]
							ul.hero__list
								li В течение 30 минут мастер будет у Вас
								li Мастера со стажем, опыт более 10 лет
								li Работаем без выходных
							img.hero__img(src="img/hero-img-main.png", alt="")
							p.hero__service Установка Windows #[strong 250&nbsp;&#8381;]
								img(src="img/hero-service-icon.png", alt="")
							p.hero__discount Скидки пенсионерам #[strong 35%]
							.hero__footer
								button.hero__button.btn.js-modal Заказать
								.hero__phones
									a.hero__phone(href="tel:+74953749832") +7 (495) 374-98-32
